package com.seismic.crm.utils;

public class ExtJSResponse {

	Boolean success;	
	String message;	
	Object obj;

	
	public ExtJSResponse(Boolean success, Object obj ,String message) {
		super();
		this.success = success;
		this.message = message;
		this.obj = obj;
	}
	
	public ExtJSResponse(Boolean success, Object obj) {
//		super();
		this.success = success;
		this.obj = obj;
	}

	public Boolean getSuccess() {
		return success;
	}

	public void setSuccess(Boolean success) {
		this.success = success;
	}

	public Object getObj() {
		return obj;
	}

	public void setObj(Object obj) {
		this.obj = obj;
	}
	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}
	
}
